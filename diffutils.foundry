[package]
name = "diffutils"
description = "GNU file comparison utilities"
homepage = "https://www.gnu.org/software/diffutils/"
license = "GPL-3.0"

[source]
type = "git"
url = "https://git.savannah.gnu.org/git/diffutils.git"
tag = "v3.10"

[build]
system = "autotools"
jobs = "auto"
inline = """
git submodule update --init --recursive
./bootstrap
./configure --prefix=${FOUNDRY_PREFIX}
make -j${FOUNDRY_JOBS}
make install
"""

[deps]

[deps.build-only]
