[package]
name = "sed"
description = "GNU stream editor"
homepage = "https://www.gnu.org/software/sed/"
license = "GPL-3.0"

[source]
type = "git"
url = "https://git.savannah.gnu.org/git/sed.git"
tag = "v4.9"

[build]
system = "autotools"
jobs = "auto"
inline = """
git submodule update --init --recursive
./bootstrap
./configure --prefix=${FOUNDRY_PREFIX}
make -j${FOUNDRY_JOBS}
make install
"""

[deps]

[deps.build-only]
