[package]
name = "tar"
description = "GNU tape archiver"
homepage = "https://www.gnu.org/software/tar/"
license = "GPL-3.0"

[source]
type = "git"
url = "https://git.savannah.gnu.org/git/tar.git"
tag = "v1.35"

[build]
system = "autotools"
jobs = "auto"
inline = """
git submodule update --init --recursive
./bootstrap
./configure --prefix=${FOUNDRY_PREFIX}
make -j${FOUNDRY_JOBS}
make install
"""

[deps]

[deps.build-only]
