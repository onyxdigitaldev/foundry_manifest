[package]
name = "m4"
description = "GNU macro processor"
homepage = "https://www.gnu.org/software/m4/"
license = "GPL-3.0"

[source]
type = "git"
url = "https://git.savannah.gnu.org/git/m4.git"
tag = "v1.4.19"

[build]
system = "autotools"
jobs = "auto"
inline = """
autoreconf -fi
./configure --prefix=${FOUNDRY_PREFIX}
make -j${FOUNDRY_JOBS}
make install
"""

[deps]

[deps.build-only]
