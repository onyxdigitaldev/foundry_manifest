[package]
name = "xz"
description = "XZ Utils compression tools and library"
homepage = "https://tukaani.org/xz/"
license = "LGPL-2.1"

[source]
type = "git"
url = "https://github.com/tukaani-project/xz.git"
tag = "v5.6.3"

[build]
system = "autotools"
jobs = "auto"
inline = """
autoreconf -fi
./configure --prefix=${FOUNDRY_PREFIX} --disable-static
make -j${FOUNDRY_JOBS}
make install
"""

[deps]

[deps.build-only]
